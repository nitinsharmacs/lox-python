class Person {
  init(name, age) {
    this.name = name;
    this.age = age;
  }

  getAge() {
    return this.age;
  }

  getName() {
    return this.name;
  }

  isEligibleToVote() {
    return this.age > 17;
  }

  isOlderThan(person) {
    return this.age > person.age;
  }
}


var person = Person("nitin", 21);

print person.getName();
print person.getAge();
print person.isEligibleToVote();


var another = Person("Hemant", 22);

print person.isOlderThan(another);
